{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "module": "es6",
    "rootDir": "./",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
