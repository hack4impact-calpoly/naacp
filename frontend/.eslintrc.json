{
  "extends": ["airbnb", "prettier", "airbnb-typescript"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [
      2,
      { "extensions": [".js", ".jsx", ".ts", ".tsx"] }
    ]
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "eslint quotes": ["error", "double"]
}
